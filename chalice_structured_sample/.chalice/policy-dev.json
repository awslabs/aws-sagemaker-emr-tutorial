{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
	"sagemaker:InvokeEndpoint"
      ],
      "Resource": [
        "arn:aws:logs:*:*:*",
        "arn:aws:sagemaker:*:*:*"
      ]
    }
  ]
}
